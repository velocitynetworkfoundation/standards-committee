{
  "title": "personName-v.1.1.schema",
  "$id": "https://velocitynetwork.foundation/schemas/personName-v.1.1",
  "allOf": [
    {
      "$ref": "../json-ld-context.schema.json"
    },
    {
      "type": "object",
      "properties": {
        "givenName": {
          "type": "string",
          "description": "The given name or first name of the person. "
        },
        "familyName": {
          "type": "string",
          "description": "The family name or surname of the person. "
        },
        "middleName": {
          "type": "string",
          "description": "The middle name of the person. "
        },
        "namePrefix": {
          "type": "string",
          "description": "A prefix to the name such as Dr or Sir. "
        },
        "nameSuffix": {
          "type": "string",
          "description": "A suffix to the name such as Jr. or Sr. "
        }                    
      }
    }
  ],
  "required": [
    "givenName",
    "familyName"
  ],
  "examples": [
    {
      "givenName": "Olivia",
      "familyName": "Hafez",
      "middleName": "Melanie",
      "namePrefix": "Dr"
    },
    {
      "givenName": "Oliver",
      "familyName": "Barrett ",
      "nameSuffix": "Jr"
    },
    {
      "givenName": "Joan Manuel ",
      "familyName": "Serrat"
 }
]
}