{
  "title": "person-v.1.1.schema",
  "$id": "https://velocitynetwork.foundation/schemas/person-v.1.1",
  "allOf": [
    {
      "$ref": "../json-ld-context.schema.json"
    },
    {
      "type": "object",
      "properties": {
        "personName": {
          "$ref": "./personName-v.1.1.schema.json"
        },
        "birthDate": {
          "type": "string",
          "format": "date",
          "description": "The date of birth of the person. "
        },
        "birthPlace": {
          "$ref": "./place-v.1.1.schema.json"
        },
        "gender": {
          "type": "string",
          "enum": [
            "Male",
            "Female"
          ],
          "description": "The gender of the person. "
        }
      }
    }
  ],
  "required": [
    "personName",
    "birthDate",
    "familyName"
  ],
  "examples": [
    {
      "personName": {
       "givenName": "Olivia",
       "familyName": "Hafez",
       "middleName": "Melanie",
       "namePrefix": "Dr"
      },
      "birthDate": "1994-06-20",
      "birthPlace": {
        "addressLocality": "Baltimore",
        "addressRegion": "US-MD",
        "addressCountry": "US"
      },
      "gender": "Female"
    },
    {
      "personName": {
       "givenName": "Oliver",
       "familyName": "Barrett ",
       "namePrefix": "string"
      },
      "birthDate": "1960-02-11",
      "birthPlace": {
       "addressLocality": "Lowell",
       "addressRegion": "US-MA",
       "addressCountry": "US"
      },
      "gender": "Male",
      "nameSuffix": "Jr"
    },
    {
      "personName": {
       "givenName": "Joan Manuel ",
       "familyName": "Serrat Teresa"
      },
      "birthDate": "1943-12-27",
      "birthPlace": {
        "addressLocality": "Barcelona",
        "addressRegion": "ES-CT",
        "addressCountry": "ES"
      },
      "gender": "Male"
    }
  ]
}