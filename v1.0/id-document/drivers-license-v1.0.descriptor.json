      "path": [
        "$.person.namePrefix"
      ],
      "schema": {
        "type": "string"
      }
    },
    {
      "label": "Name suffix",
      "path": [
        "$.person.nameSuffix"
      ],
      "schema": {
        "type": "string"
      }
    },
    {
