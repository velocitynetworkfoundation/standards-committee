{
  "title": {
    "path": [
      "$.name"
    ],
    "schema": {
      "type": "string"
    },
    "fallback": "Driver's License"
  },
  "subtitle": {
    "path": [
      "$.authority.name"
    ],
    "schema": {
      "type": "string"
    },
    "fallback": "-"
  },
  "summary_detail": {
    "path": [
      "$.identifier"
    ],
    "schema": {
      "type": "string"
    },
    "fallback": "-"
  },
  "description": {
    "text": "Driver's license"
  },
  "logo": {
    "path": [
      "$.authority.image"
    ],
    "schema": {
      "type": "string",
      "format": "uri"
    }
  },
  "properties": [
    {
      "label": "License number",
      "path": [
        "$.identifier"
      ],
      "schema": {
        "type": "string"
      }
    },
    {
      "label": "Country of issuing authority",
      "path": [
        "$.authority.place.addressCountry"
      ],
      "schema": {
        "type": "string"
      }
