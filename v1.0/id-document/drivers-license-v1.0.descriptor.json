      ],
      "schema": {
        "type": "string"
      }
    },
    {
      "label": "Address locality",
      "path": [
        "$.addressLocality"
      ],
      "schema": {
        "type": "string"
      }
    },
    {
      "label": "Region or state",
      "path": [
        "$.addressRegion"
      ],
      "schema": {
        "type": "string"
      }
    },
    {
      "label": "Postal code",
      "path": [
        "$.postCode"
      ],
      "schema": {
        "type": "string"
      }
    },
    {
      "label": "Country",
      "path": [
        "$.addressCountry"
      ],
      "schema": {
        "type": "string"
      }
    }
