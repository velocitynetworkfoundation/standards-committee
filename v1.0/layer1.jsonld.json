{
  "@context": {
    "schema": "http://schema.org/",
    "xsd": "https://www.w3.org/2001/XMLSchema#",
    "vnf": "https://velocitynetwork.foundation/contexts#",
    "obiv2": "https://w3id.org/openbadges/v2#",
    "ceterms": "https://credreg.net/ctdl/terms/",
    "Address": {
      "@id": "vnf:PostalAddress",
      "@context": {
        "streetAddress": "schema:streetAddress",
        "addressLocality": "schema:addressLocality",
        "addressRegion": "schema:addressRegion",
        "postCode": "schema:postalCode",
        "addressCountry": "schema:addressCountry"
      }
    },
    "Place": {
      "@id": "vnf:Place",
      "@context": {
        "name": "schema:name",
        "addressLocality": "schema:addressLocality",
        "addressRegion": "schema:addressRegion",
        "addressCountry": "schema:addressCountry"
      }
    },
    "Validity": {
      "@id": "vnf:Validity",
      "@context": {
        "firstValidFrom": {
          "@id": "vnf:firstValidFrom",
          "@type": "schema:Date"
        },
        "validFrom": "schema:validFrom",
        "validUntil": "schema:validUntil",
        "validIn": "vnf:Place"
      }
    },
    "Person": {
      "@id": "vnf:Person",
      "@context": {
        "givenName": "schema:givenName",
        "familyName": "schema:familyName",
        "middleName": "schema:additionalName",
        "namePrefix": "schema:honorificPrefix",
        "nameSuffix": "schema:honorificSuffix",
        "birthDate": "schema:birthDate",
        "birthPlace": {
          "@id":"vnf:birthPlace",
          "@type": "@id"
        },
        "gender": "schema:gender"
      }
    },
    "Organization": {
      "@id": "vnf:Organization",
      "@context": {
        "name": "schema:name",
        "identifier": {
          "@id": "schema:identifier",
          "@type": "@id"
        },
        "place": {
          "@id":"vnf:place",
          "@type": "@id"
        }
      }
    },
    "AlignmentObject": {
      "@id": "vnf:AlignmentObject",
      "@context": {
        "targetCode": {
          "@id": "obiv2:targetCode",
          "@type": "xsd:string"
        },
        "targetDescription": "schema:targetDescription",
        "targetFramework": "schema:targetFramework",
        "targetName": "schema:targetName",
        "targetType": {
          "@id": "obiv2:targetType",
          "@type": "xsd:string"
        },
        "targetUrl": {
          "@id": "schema:targetUrl",
          "@type": "xsd:anyURI"
        }
      }
    },
    "Score": {
      "@id": "vnf:Score",
      "@context": {
        "scoreMethod": "vnf:ScoreMethod",
        "scoreValue": {
          "@id": "vnf:scoreValue",
          "@type": "xsd:string"
        },
        "result": {
          "@id": "vnf:result",
          "@type": "xsd:string"
        },
        "passFail":  {
          "@id": "vnf:passFail",
          "@type": "xsd:string"
        }
      }
    },
    "ScoreMethod": {
      "@id": "vnf:ScoreMethod",
      "@context": {
        "scoreMethodType": "ceterms:scoringMethodType",
        "scoreMethodDescription": "ceterms:scoringMethodDescription",
        "alignment": { "@id": "vnf:AlignmentObject", "@container": "@set" },
        "passingScore": "ceterms:requires"
      }
    },
    "Email": {
      "@id": "vnf:Email",
      "@context": {
        "email": "schema:email"
      }
    },
    "Phone": {
      "@id": "vnf:Phone",
      "@context": {
        "phone": "schema:telephone"
      }
    },
    "IdDocument":  {
      "@id": "vnf:IdDocument",
      "@context": {
        "documentType": "schema:additionalType",
        "postalAddress": { "@id": "vnf:Address", "@type": "@id" },
        "authority": { "@id": "vnf:Organization", "@type": "@id" },
        "validity": { "@id": "vnf:Validity", "@type": "@id" },
        "person": { "@id": "vnf:Person", "@type": "@id" },
        "identifier": {
          "@id": "schema:identifier",
          "@type": "@id"
        },
        "nationality": "schema:addressCountry"
      }
    },
    "Passport":  {
      "@id": "vnf:Passport",
      "@context": {
        "documentCode": "schema:additionalType",
        "authority": { "@id": "vnf:Organization", "@type": "@id" },
        "validity": { "@id": "vnf:Validity", "@type": "@id" },
        "person": { "@id": "vnf:Person", "@type": "@id" },
        "identifier": {
          "@id": "schema:identifier",
          "@type": "@id"
        },
        "nationality": "schema:addressCountry"
      }
    },
    "DriversLicense": {
      "@id": "vnf:DriversLicense",
      "@context": {
        "authority": { "@id": "vnf:Organization", "@type": "@id" },
        "validity": { "@id": "vnf:Validity", "@type": "@id" },
        "person": { "@id": "vnf:Person", "@type": "@id" },
        "identifier": {
          "@id": "schema:identifier",
          "@type": "@id"
        },
        "nationality": "schema:addressCountry",
        "address": { "@id": "vnf:Address", "@type": "@id" },
        "description": "schema:description",
        "endorsementCode": { "@id": "xsd:string", "@container": "@set" },
        "restrictionCode": { "@id": "xsd:string", "@container": "@set" },
        "alignment": { "@id": "vnf:AlignmentObject", "@container": "@set" }
      }
    },
    "Employment-1.0": {
      "@id": "vnf:Employment-1.0",
      "@context": {
        "legalEmployer": { "@id": "vnf:Organization", "@type": "@id" },
        "role": "schema:roleName",
        "description": "schema:description",
        "employmentType": "schema:employmentType",
        "startDate": { "@id": "vnf:startDate", "@type": "schema:Date"},
        "endDate": { "@id": "vnf:endDate", "@type": "schema:Date"},
        "place": "vnf:place",
        "alignment": { "@id": "vnf:AlignmentObject", "@container": "@set" }
      }
    },
    "EducationDegree-1.0": {
      "@id": "vnf:EducationDegree-1.0",
      "@context": {
        "institution": { "@id": "vnf:Organization", "@type": "@id" },
        "school": { "@id": "vnf:Organization", "@type": "@id" },
        "programName": "schema:name",
        "programType": "schema:programType",
        "programMode": "schema:educationalProgramMode",
        "degreeName": "schema:educationalCredentialAwarded",
        "degreeMajor": "ceterms:degreeMajor",
        "degreeMinor": "ceterms:degreeMinor",
        "description": "schema:description",
        "alignment": { "@id": "vnf:AlignmentObject", "@container": "@set" },
        "registrationDate": { "@id": "vnf:registrationDate", "@type": "schema:Date"},
        "conferredDate": { "@id": "vnf:conferredDate", "@type": "schema:Date"},
        "startDate": { "@id": "vnf:startDate", "@type": "schema:Date"},
        "endDate": { "@id": "vnf:endDate", "@type": "schema:Date"},
        "honors": "xsd:string",
        "grade": { "@id": "vnf:Score" }
      }
    },
    "Course-1.0": {
      "@id": "vnf:Course-1.0",
      "@context": {
        "provider": { "@id": "vnf:Organization", "@type": "@id" },
        "contentProvider": { "@id": "vnf:Organization", "@type": "@id" },
        "courseName": "schema:educationalCredentialAwarded",
        "courseCode": "schema:courseCode",
        "courseType": "schema:programType",
        "courseMode": "schema:educationalProgramMode",
        "courseLevel": "schema:educationalLevel",
        "courseDuration": "schema:timeToComplete",
        "description": "schema:description",
        "alignment": { "@id": "vnf:AlignmentObject", "@container": "@set" },
        "grade": { "@id": "vnf:Score" },
        "registrationDate": { "@id": "vnf:registrationDate", "@type": "schema:Date"},
        "startDate": { "@id": "vnf:startDate", "@type": "schema:Date"},
        "completionDate": { "@id": "vnf:completionDate", "@type": "schema:Date"}
      }
    },
    "AssessmentDimension": {
      "@id": "vnf:AssessmentDimension",
      "@context": {
        "name": "schema:name",
        "assesses": "schema:assesses",
        "description": "schema:description",
        "assessmentLevel": "schema:educationalLevel",
        "assessmentMethod": "ceterms:assessmentMethodDescription",
        "alignment": { "@id": "vnf:AlignmentObject", "@container": "@set" },
        "score": { "@id": "vnf:Score" },
        "assessmentDate": { "@id": "vnf:assessmentDate", "@type": "schema:Date"}
      }
    },
    "Assessment-1.0": {
      "@id": "vnf:Assessment-1.0",
      "@context": {
        "name": "schema:name",
        "authority": { "@id": "vnf:Organization", "@type": "@id" },
        "assesses": "schema:assesses",
        "description": "schema:description",
        "assessmentLevel": "schema:educationalLevel",
        "assessmentMethod": "ceterms:assessmentMethodDescription",
        "alignment": { "@id": "vnf:AlignmentObject", "@container": "@set" },
        "score": { "@id": "vnf:Score" },
        "assessmentDate": { "@id": "vnf:assessmentDate", "@type": "schema:Date"},
        "assessmentDimensions": { "@id": "vnf:AssessmentDimension", "@container": "@list" }
      }
    },
    "Certification-1.0": {
      "@id": "vnf:Certification-1.0",
      "@context": {
        "name": "schema:name",
        "authority": { "@id": "vnf:Organization", "@type": "@id" },
        "description": "schema:description",
        "identifier": {
          "@id": "schema:identifier",
          "@type": "@id"
        },
        "validity": { "@id": "vnf:Validity", "@type": "@id" },
        "alignment": { "@id": "vnf:AlignmentObject", "@container": "@set" }
      }
    },
    "License-1.0": {
      "@id": "vnf:License-1.0",
      "@context": {
        "name": "schema:name",
        "authority": { "@id": "vnf:Organization", "@type": "@id" },
        "description": "schema:description",
        "identifier": {
          "@id": "schema:identifier",
          "@type": "@id"
        },
        "validity": { "@id": "vnf:Validity", "@type": "@id" },
        "alignment": { "@id": "vnf:AlignmentObject", "@container": "@set" },
        "endorsementCode": { "@id": "xsd:string", "@container": "@set" },
        "restrictionCode": { "@id": "xsd:string", "@container": "@set" }
      }
    }
  }
}