{
  "title": {
    "path": [
      "$.authorityName.localized.en", "$.credentialSubject.authorityName.localized.en"
    ],
    "schema": {
      "type": "string"
    },
    "fallback": "-"
  },
  "subtitle": {
    "path": [
      "$.name.localized.en", "$.credentialSubject.name.localized.en"
    ],
    "schema": {
      "type": "string"
    },
    "fallback": "-"
  },
  "summary_detail": {
    "path": [
      "$.type.localized.en", "$.credentialSubject.type.localized.en"
    ],
    "schema": {
      "type": "string"
    }
  },
  "description": {
    "text": "Velocity Network certification credentials"
  },
  "properties": [
    {
      "label": "Number",
      "path": [
        "$.identifier", "$.credentialSubject.identifier"
      ],
      "schema": {
        "type": "string"
      }
    },
    {
      "label": "Date",
      "path": [
        "$.certificationDate", "$.credentialSubject.certificationDate"
      ],
      "schema": {
        "type": "string",
        "format": "date"
      }
    },
    {
      "label": "Renewal date",
      "path": [
        "$.datesRenewed", "$.credentialSubject.datesRenewed"
      ],
      "schema": {
        "type": "string",
        "format": "date"
      }
    },
    {
      "label": "Alignment",
      "path": [
        "$.alignment[0].targetName", "$.credentialSubject.alignment[0].targetName"
      ],
      "schema": {
        "type": "string"
      }
    },
    {
      "label": "Alignment URL",
      "path": [
        "$.alignment[0].targetUrl", "$.credentialSubject.alignment[0].targetUrl"
      ],
      "schema": {
        "type": "string",
        "format": "uri"
      }
    },
    {
      "label": "Alignment framework",
      "path": [
        "$.alignment[0].targetFramework", "$.credentialSubject.alignment[0].targetFramework"
      ],
      "schema": {
        "type": "string"
      }
    }
  ]
}