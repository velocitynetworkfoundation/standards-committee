 {
  "display": {
    "title": {
      "path": [
        "$.authority.name"
      ],
      "schema": {
        "type": "string"
      },
      "fallback": "-"
    },
    "subtitle": {
      "path": [
        "$.name"
      ],
      "schema": {
        "type": "string"
      },
      "fallback": "-"
